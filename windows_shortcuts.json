{
  "title": "Windows Shortcuts",
  "rules": [
    {
      "description": "Home [Unless Terminal]",
      "manipulators": [
        {
          "from": {
            "key_code": "home",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
              "key_code": "left_arrow",
              "modifiers": ["command"]
          }],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.apple\\.Terminal$",
                "^com\\.googlecode\\.iterm2$"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Home (Ctrl)",
      "manipulators": [
        {
          "from": {
            "key_code": "home",
            "modifiers": { "mandatory": ["control"], "optional": ["any"] }
          },
          "to": [{
            "key_code": "up_arrow",
            "modifiers": ["command"]
          }],
          "type": "basic"
        }
      ]
    },
